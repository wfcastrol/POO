public class TestRational {
    public static void main (String [] args) {
	Rational rat = new Rational (2,3);
	Rational rat2 = new Rational (1,3);
	System.out.println(rat.toString());
	rat.mult (rat2);
	System.out.println(rat.toString());
	rat.sum (rat2);
	System.out.println(rat.toString());
    }
}